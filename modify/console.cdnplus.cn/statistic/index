        summary: function(){
          $.getJSON('/statistic/summary/', params).done(function(res){
            var data = res.data;
            $('#count').text(data.count);
            $('#hit_rate').text(data.hit_rate);
            $('#max_bandwidth').text(data.max_bindwidth);
            $('#flow').text(data.flow);
          })
        }
      };
	  //按钮点击事件
      bandwith_init = function(e){
        overview.summary();
        overview.bandwidth();
        $(e).parent().parent().find('button').removeClass('btn-success');
        $(e).addClass('btn-success');
      }
      flow_init = function(e){
        overview.summary();
        overview.flow();
        $(e).parent().parent().find('button').removeClass('btn-success');
        $(e).addClass('btn-success');
      }
      visit_init = function(e){
        overview.summary();
        overview.visit();
        $(e).parent().parent().find('button').removeClass('btn-success');
        $(e).addClass('btn-success');
      }
      //触发默认数据
      $('#search_type button').eq(0).click();
      //搜索按钮
      btnSearch = function(){
        $('#search_type button.btn-success').click();
      }
      //赋值时间
      setTime = function(e, start, end){
        $(e).parent().parent().find('button').removeClass('btn-success');
        $(e).addClass('btn-success');
        //自定义去掉样式
        $('#customTime').removeClass('btn-success');
		
		var now = new Date();
		var start_time = new Date();
		var t_s = now.getTime();
		start_time.setTime(t_s - 1000 * end);
		
        params.start_time = start_time.format("yyyyMMddhhmm");
        params.end_time = now.format("yyyyMMddhhmm");
        btnSearch();
        timeText = $(e).text()
        $('.summaryTime').text(timeText);
      }
      setCustomTime = function(){
        var start_str = $('#dpStart').val();
        var end_str = $('#dpEnd').val();
        var start = DateTime(start_str);
        var end = DateTime(end_str);
        params.start_time = start;
        params.end_time = end;
        var c = '从'+start_str+'至'+end_str;
        var e = $('#customTime');
        e.html('<small title="'+c+'">'+c+'</samll>');
        $(e).parent().parent().find('button').removeClass('btn-success');
        $(e).addClass('btn-success');
        btnSearch();
        timeText = '自定义';
        $('.summaryTime').text(timeText);
		//自定义去掉样式
        $('#customTime').removeClass('btn-success');
      }
    })
    </script>


        
        <br class="clear"/>
    </div>
    <!-- END Content -->

    
    <div id="footer"></div>
    
</div>
<!-- END Container -->
