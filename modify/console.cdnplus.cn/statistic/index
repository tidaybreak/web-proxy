	  overview = {
	    bandwidth: function (){
			    var chart = echarts.init(document.getElementById('flot_overview'), 'shine')
			    chart.showLoading({'effect': 'bubble'});
			    $.getJSON('/statistic/flow-bandwidth/', params).done(function(res){
			        chart.hideLoading();
			        var times = [];
			        var values = [];
			        res.data.map(function(value, index){
			          times.push(value[0]);
			          values.push(value[1]);
                    })

			        //判别单位
                    var max_value = Math.max.apply(null, values);
                    if(max_value < 1024){
                      var uint = 'GB';
                      var uint_num = 1;
                    }else if( 1024 <= max_value && max_value <= 1024*1024 ){
                      var uint = 'Kbps';
                      var uint_num = 1024;
                    }else{
                      var uint = 'Mbps';
                      var uint_num = 1024 * 1024;
                    }
                    // 修改配置
                    line_option.xAxis.data = times;
                    line_option.title = [
                        {
                        text: '带宽图',
                        subtext: '近 '+timeText,
                        left: '90%',
                        top: '1%'
                      },
                      {
                      text: '单位：' + uint,
                      borderColor: '#999',
                      borderWidth: 1,
                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }];
                    line_option.series.name = '带宽(' + uint + ')'
			        var data = [];
			        for(var index in values){
			          data.push((values[index]/uint_num).toFixed(1))
                    }
			        line_option.series.data = data;
			        chart.setOption(line_option)
                })
		},
        flow: function (){
			if($('#flot_overview').length) {
			    var chart = echarts.init(document.getElementById('flot_overview'), 'shine')
			    chart.showLoading({'effect': 'bubble'});
			    $.getJSON('/statistic/node-flow/', params).done(function(res){
			        chart.hideLoading();
			        var times = [];
			        var values = [];
			        res.data.map(function(value, index){
			          times.push(value[0]);
			          values.push(value[1]);
                    })

			        //判别单位
                    var max_value = Math.max.apply(null, values);
                    if(max_value < 1024){
                      var uint = '字节';
                      var uint_num = 1;
                    }else if( 1024 <= max_value && max_value <= 1024*1024 ){
                      var uint = 'KB';
                      var uint_num = 1024;
                    }else{
                      var uint = 'MB';
                      var uint_num = 1024 * 1024;
                    }
					uint = 'BG'
                    line_option.xAxis.data = times;
                    line_option.title = [
                        {
                        text: '流量图',
                        subtext: '近'+timeText,
                        left: '90%',
                        top: '1%'
                      },
                      {
                      text: '单位：' + uint,
                      borderColor: '#999',
                      borderWidth: 1,
                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }];
                    line_option.series.name = '流量(' + uint + ')'

			        var data = [];
			        for(var index in values){
			          data.push((values[index]).toFixed(1))
                    }
			        line_option.series.data = data;
			        chart.setOption(line_option)
                })
			}
		},
        visit: function (){
			if($('#flot_overview').length) {
			    var chart = echarts.init(document.getElementById('flot_overview'), 'shine')
			    chart.showLoading({'effect': 'bubble'});
			    $.getJSON('/statistic/node-visit/', params).done(function(res){
			        chart.hideLoading();
			        var times = [];
			        var values = [];
			        res.data.map(function(value, index){
			          times.push(value[0]);
			          values.push(value[1]);
                    })

			        //判别单位
                    var max_value = Math.max.apply(null, values);
                    line_option.xAxis.data = times;
                    line_option.title = [
                        {
                        text: '访问次数',
                        subtext: '近'+timeText,
                        left: '90%',
                        top: '1%'
                      },
                      {
                      text: '单位：次',
                      borderColor: '#999',
                      borderWidth: 1,
                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }];
                    line_option.series.name = '访问(' + '次' + ')';

			        var data = [];
			        for(var index in values){
			          data.push(parseInt(values[index]/1))
                    }
			        line_option.series.data = data;
			        chart.setOption(line_option)
                })
			}
		},
        bandwidth_ranking: function(){
          var bar = echarts.init(document.getElementById('chart_bars'), 'shine');
          bar.showLoading({'effect': 'bubble'});
          $.getJSON('/statistic/node-bandwidth-ranking/', params).done(function(res){
            bar.hideLoading();
            var x_values = [];
            var y_values = [];
            res.data.map(function(value, index){
              x_values.push(value[1]);
              y_values.push(value[0])
            })
            var max_value = Math.max.apply(null, x_values);
            if(max_value < 1024){
              var uint = 'GB';
              var uint_num = 1;
            }else if( 1024 <= max_value && max_value <= 1024*1024 ){
              var uint = 'Kbps';
              var uint_num = 1024;
            }else{
              var uint = 'Mbps';
              var uint_num = 1024 * 1024;
            }
            var data = [];
            for(var index in x_values){
              data.push(parseInt(x_values[index]/uint_num))
            }
            bar_option.title = [
                        {
                        text: '节点带宽排名',
                        subtext: '近'+timeText,
                        left: '90%',
                        top: '1%'
                      },
                      {
                      text: '单位：' + uint,
                      borderColor: '#999',
                      borderWidth: 1,
                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }];
            bar_option.yAxis.data = y_values;
            bar_option.series.data = data;
            bar_option.series.name = '带宽(' + uint + ')';
            bar.setOption(bar_option);
          })

        },
