	  overview = {
        url_bandwidth_ranking: function(){
          var bar = echarts.init(document.getElementById('status_code_line'), 'shine');
          bar.showLoading({'effect': 'bubble'});
          params.type = 'bandwidth';
          $.getJSON('/statistic/query-domain-url-ranking/', params).done(function(res){
            bar.hideLoading();
            var x_values = [];
            var y_values = [];
            res.data.map(function(value, index){
              x_values.push(value[1]);
              y_values.push(value[0])
            })
            var max_value = Math.max.apply(null, x_values);
            console.log(max_value, 321)
            if(max_value < 1024){
              var uint = 'Bps';
              var uint_num = 1;
            }else if( 1024 <= max_value && max_value <= 1024*1024 ){
              var uint = 'Kbps';
              var uint_num = 1024;
            }else{
              var uint = 'Mbps';
              var uint_num = 1024 * 1024;
            }
            var data = [];
            for(var index in x_values){
              data.push(parseInt(x_values[index]/uint_num))
            }
            bar_option.title = [
                        {
                        text: '带宽排名',
                        subtext: '近'+timeText,
                        left: '90%',
                        top: '1%'
                      },
                      {
                      text: '单位：' + uint,
                      borderColor: '#999',
                      borderWidth: 1,
                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }];
            bar_option.yAxis.data = y_values;
            bar_option.series.data = data;
            bar_option.series.name = '带宽(' + uint + ')';
            bar.setOption(bar_option);
          })

        },
        url_flow_ranking: function(){
          var bar = echarts.init(document.getElementById('status_code_line'), 'shine');
          bar.showLoading({'effect': 'bubble'});
          params.type = 'flow';
          $.getJSON('/statistic/query-domain-url-ranking/', params).done(function(res){
            bar.hideLoading();
            var x_values = [];
            var y_values = [];
            res.data.map(function(value, index){
              x_values.push(value[1]);
              y_values.push(value[0])
            })
            var max_value = Math.max.apply(null, x_values);
            if(max_value < 1024){
              var uint = 'B';
              var uint_num = 1;
            }else if( 1024 <= max_value && max_value <= 1024*1024 ){
              var uint = 'KB';
              var uint_num = 1024;
            }else{
              var uint = 'MB';
              var uint_num = 1024 * 1024;
            }
            var data = [];
            for(var index in x_values){
              data.push(parseInt(x_values[index]/uint_num))
            }
            bar_option.title = [
                        {
                        text: '流量排名',
                        subtext: '近'+timeText,
                        left: '90%',
                        top: '1%'
                      },
                      {
                      text: '单位：' + uint,
                      borderColor: '#999',
                      borderWidth: 1,
                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }];
            bar_option.yAxis.data = y_values;
            bar_option.series.data = data;
            bar_option.series.name = '流量(' + uint + ')';
            bar.setOption(bar_option);
          })

        },
