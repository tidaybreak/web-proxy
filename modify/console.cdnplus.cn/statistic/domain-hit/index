                      textStyle: {
                        fontSize: 12
                      },
                      left: '2%',
                      top: '3%'
                    }],
                  tooltip : {
                    trigger: 'axis',
                    axisPointer: {
                      type: 'cross',
                      animation: false,
                      label: {
                        backgroundColor: '#505765'
                      }
                    }
                  },
                  legend: {
                    data:['带宽'],
                    x: 'left'
                  },
                  dataZoom: [
                    {
                      show: true,
                      realtime: true,
                      start: 0,
                      end: 1000
                    },
                    {
                      type: 'inside',
                      realtime: true,
                      start: 0,
                      end: 1000
                    }
                  ],
                  xAxis : {
                      type : 'category',
                      boundaryGap : true,
                      axisLine: {onZero: false},
                    },
                  yAxis: [
                    {
                      splitNumber: 7,
                      type: 'value'
                    }
                  ],
                  series:
                    {
                      name:'命中率',
                      type:'line',
                      animation: false,
                      smooth:true,
                      areaStyle: {
                        normal: {},
                        color:'rgba(0,190,0,0.7)'
                      },
                      itemStyle : {
                        normal : {
